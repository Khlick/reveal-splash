{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "browsers": [
          "last 2 Chrome versions",
          "last 2 Safari versions",
          "last 2 iOS versions",
          "last 2 Firefox versions",
          "last 2 Edge versions"
        ]
      },
      "useBuiltIns": "usage",
      "corejs": 3,
      "modules": false
    }]
  ],
  "plugins": [
    "transform-html-import-to-string"
  ]
}
